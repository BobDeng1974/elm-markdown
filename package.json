{
  "name": "elm-markdown-parser",
  "version": "1.0.0",
  "description": "Markdown parsing in pure elm.",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rm -r dist && parcel build src/index.html --public-url ./",
    "test": "elm-test",
    "autotest": "elm-test --watch tests/**/*.elm",
    "diff": "git show HEAD:spec-results.json > git-head-spec-results.json && json-diff git-head-spec-results.json spec-results.json",
    "specs:marked": "cd test && npx elm make OutputMarkdownHtml.elm --output elm.js && cd .. && jasmine --config=jasmine.json test/specs/**/*-spec.js"
  },
  "author": "Dillon Kearns",
  "dependencies": {},
  "devDependencies": {
    "@markedjs/html-differ": "^3.0.0",
    "elm": "^0.19.0-no-deps",
    "elm-format": "^0.8.2",
    "elm-hot": "^1.0.1",
    "elm-test": "^0.19.0-rev6",
    "front-matter": "^3.0.2",
    "jasmine": "^3.4.0",
    "json-diff": "^0.5.4",
    "json-stable-stringify": "^1.0.1",
    "node-elm-compiler": "^5.0.3",
    "parcel-bundler": "^1.12.0"
  }
}
